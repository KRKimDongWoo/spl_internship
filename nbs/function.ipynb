    "#export\n",
    "def ceil_power_of_two(x):\n",
    "    x = ceil(x) - 1\n",
    "    res = 0\n",
    "    while x > 0:\n",
    "        x = x // 2\n",
    "        res = res + 1\n",
    "        \n",
    "    return 2**(res)"
   ]
  },
